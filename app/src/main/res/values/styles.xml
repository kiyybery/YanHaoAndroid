<resources>

    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="bgimg">
        <item name="android:background">@drawable/bg_repeat</item>
    </style>

    <style name="Widget.IOSched.EditText" parent="Widget.AppCompat.EditText">
        <item name="android:background">@color/white</item>
        <item name="android:textCursorDrawable">@drawable/edittext_cursor_color</item>
    </style>

    <style name="UserCenter" />

    <style name="UserCenter.Indicator">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:src">@drawable/line_icon</item>
    </style>

    <style name="Snachat" />

    <style name="Snapchat.Detail" parent="Snachat"></style>

    <style name="Snapchat.Detail.Comment" parent="Snapchat.Detail" />

    <style name="Snapchat.Detail.Comment.Item2" parent="Snapchat.Detail.Comment">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/yr_text_dark</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">8dp</item>

    </style>

    <style name="Snapchat.Detail.Comment.Item3" parent="Snapchat.Detail.Comment">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">8dp</item>

    </style>
    <!--Welcome-->
    <!--登陆,注册-->
    <style name="Account">

    </style>

    <style name="Account.TextView">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">82dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_text_light</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">20dp</item>
    </style>

    <style name="Account.EditText" parent="Widget.IOSched.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_text_dark</item>
        <item name="android:textColorHint">@color/yr_text_hint</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">12dp</item>
    </style>

    <style name="Settings.EditText" parent="Widget.IOSched.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_text_dark</item>
        <item name="android:textColorHint">@color/yr_text_hint</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>

        <item name="android:layout_gravity">left|center_vertical</item>

    </style>

    <!--Match Dialog-->
    <style name="cart_dialog" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="commonAvatar">
        <item name="android:layout_width">@dimen/avatar_worksList</item>
        <item name="android:layout_height">@dimen/avatar_worksList</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">@dimen/worklistAvatarMarginLeft</item>
        <item name="android:clickable">true</item>
        <item name="android:src">@drawable/avatar_default</item>
    </style>

    <style name="MyProfile" />

    <style name="MyProfile.Text">
        <item name="android:background">@color/yr_background_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_title_secondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:layout_gravity">left</item>
    </style>

    <style name="MyProfile.Text.UC">
        <item name="android:background">@color/yr_background_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">@color/yr_title_secondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">16.5dp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:drawableLeft">@drawable/uc_myask</item>
        <item name="android:drawableRight">@drawable/arrow_right_uc</item>
    </style>

    <style name="MyProfile.Text.UC1">
        <item name="android:background">@color/yr_background_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">@color/yr_title_secondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">16.5dp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:drawableLeft">@drawable/uc_mytest</item>
        <item name="android:drawableRight">@drawable/arrow_right_uc</item>
    </style>

    <style name="MyProfile.Text.UC2">
        <item name="android:background">@color/yr_background_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">@color/yr_title_secondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">16.5dp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:drawableLeft">@drawable/uc_mycollection</item>
        <item name="android:drawableRight">@drawable/arrow_right_uc</item>
    </style>

    <style name="MyProfile.Text.UC3">
        <item name="android:background">@color/yr_background_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">@color/yr_title_secondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">16.5dp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:drawableLeft">@drawable/uc_asknote</item>
        <item name="android:drawableRight">@drawable/arrow_right_uc</item>
    </style>

    <style name="A1_Font">
        <item name="android:textColor">#333333</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">-1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>

    <style name="Consultant" />

    <style name="Consultant.Text">
        <item name="android:background">@color/yr_background_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_title_secondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:drawableRight">@drawable/icon_arrow_right</item>
    </style>

    <style name="UserCenter.Mine" parent="UserCenter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="UserCenter.Mine.tvNick">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/profileTopWidgetMarginLeft</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_text_dark</item>
        <item name="android:singleLine">true</item>
    </style>

    <!--选择-->
    <style name="Select" />

    <style name="Select.ItemText">
        <item name="android:layout_weight">2</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/yr_text_dark</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Select.TitleText">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/yr_text_dark</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="SelectCheckedButton" parent="android:Widget.ImageButton">
        <item name="android:visibility">visible</item>
        <item name="android:src">@drawable/checkbox_open</item>
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <dimen name="profileTopWidgetMarginLeft">30dp</dimen>

    <!--ios dialog-->
    <style name="mystyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/bg</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!--toolbar-->
    <style name="ToolBarStyle.Base" parent="ThemeOverlay.AppCompat.ActionBar">
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
    </style>

    <style name="ToolBarStyle" parent="ToolBarStyle.Base" />

    <!--popupwindow-->
    <style name="AnimTools" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_in</item>
        <item name="android:windowExitAnimation">@anim/push_out</item>
    </style>

    <!--分享-->
    <style name="shareButtonStyle" parent="Base.Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">8dp</item>
    </style>

    <style name="FrameworkRoot.Theme" parent="Theme.AppCompat.Light.NoActionBar" />

    <style name="Theme" parent="FrameworkRoot.Theme">
        <!-- Only needed for pre-L -->
        <item name="android:editTextBackground">@color/white</item>
        <item name="android:editTextStyle">@style/Widget.IOSched.EditText</item>
        <item name="android:textCursorDrawable">@drawable/edittext_cursor_color</item>
    </style>

    <style name="Theme.IOSched.Base" parent="Theme">
        <item name="colorPrimary">@color/theme_primary</item>
        <item name="colorPrimaryDark">@color/theme_primary_light</item>
        <!--0615 影响了radio button ,edit text 高亮-->
        <item name="colorControlActivated">@color/yr_icon_blending</item>
        <!--0615 影响了点击某些button 按下时的状态-->
        <item name="colorControlHighlight">@color/yr_pressed</item>

        <item name="android:textColorLink">@color/yr_pressed</item>


        <!--   <item name="android:windowContentOverlay">@null</item>-->
        <item name="android:windowBackground">@android:color/white</item>
        <!--  <item name="android:fastScrollThumbDrawable">@drawable/scrollbar_thumb</item>
          <item name="android:fastScrollTrackDrawable">@drawable/scrollbar_track</item>
          <item name="android:scrollbarThumbVertical">@drawable/scrollbar_thumb</item>
          <item name="android:scrollbarTrackVertical">@drawable/scrollbar_track</item>-->
    </style>

    <style name="Theme.IOSched" parent="Theme.IOSched.Base">
        <item name="actionMenuTextColor">@color/yr_text_dark</item>
        <item name="android:actionMenuTextColor">@color/red</item>

    </style>

    <style name="Theme.IOSched.NoActionbar" parent="@style/Theme.IOSched">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ThemeTranslucent" parent="Theme.IOSched.NoActionbar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
</resources>
